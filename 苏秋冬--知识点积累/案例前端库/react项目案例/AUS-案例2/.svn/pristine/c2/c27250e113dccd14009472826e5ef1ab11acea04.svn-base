    cursor: pointer;
}
.tree-hit {
    cursor: pointer;
}
.tree-expanded,
.tree-collapsed,
.tree-folder,
.tree-file,
.tree-checkbox,
.tree-indent {
    display: inline-block;
    width: 16px;
    height: 18px;
    vertical-align: top;
    overflow: hidden;
}
.tree-expanded {
    background: url("../img/tree_icons.png") no-repeat -18px 0px;
}
.tree-expanded-hover {
    background: url("../img/tree_icons.png") no-repeat -50px 0px;
}
.tree-collapsed {
