*/
.Associated{
    margin-right: 0;
}
.progress-bar{
    -webkit-transition: none;
}
.Refresh-dialog{
    width: 100%;
    height: 100%;
    margin: 0 auto;
}
.Refresh-content{
    width: 350px;
    height: 20px;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}
.modal-footer {
     border-top: none;
}
