.tree-dnd-icon {
    display: inline-block;
    position: absolute;
    width: 16px;
    height: 18px;
    left: 2px;
    top: 50%;
    margin-top: -9px;
}
.tree-dnd-yes {
    background: url("../img/tree_icons.png") no-repeat -256px 0;
}
.tree-dnd-no {
    background: url('../img/tree_icons.png') no-repeat -256px -18px;
}
.tree-node-top {
    border-top: 1px dotted red;
}
.tree-node-bottom {
    border-bottom: 1px dotted red;
}
.tree-node-append .tree-title {
    border: 1px dotted red;
}
.tree-editor {
    border: 1px solid #ddd;
    font-size: 12px;
    line-height: 16px;
    padding: 0 4px;
    margin: 0;
    width: 80px;
    outline-style: none;
    vertical-align: top;
    position: absolute;
    top: 0;
}
.tree-node-proxy {
    background-color: #fff;
    color: #444;
    border-color: #ddd;
}
/*
tree样式重写
*/
.tree-file，.tree-folder-open，.tree-folder,.tree-node-hidden  {
    background: none;
}
.tree-node-selected {
    background:  rgb(58, 156, 143) !important;
    color: #fff !important;
}
.tree-node-hover {
    background: #f9f9f9;
    color: #000;
}
.tree-expanded {
    background-position: -18px 4px;
}
.tree-expanded, .tree-collapsed, .tree-folder, .tree-file, .tree-checkbox, .tree-indent {

    height: 25px;

}
#tree{
    max-height: 400px;
    min-height: 400px;
    overflow: auto;
    margin: 0;
    padding: 0;
    list-style-type: none;
}
/*
 #Refresh-model{
